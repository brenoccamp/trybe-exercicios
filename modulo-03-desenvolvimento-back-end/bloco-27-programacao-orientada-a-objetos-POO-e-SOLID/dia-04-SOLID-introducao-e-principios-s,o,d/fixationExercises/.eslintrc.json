{
  "root": true,
  "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "trybe-backend"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "ignorePatterns": ["tests/", "node_modules/"],
  "env": { "es2021": true },
  "plugins": ["@typescript-eslint"]
}